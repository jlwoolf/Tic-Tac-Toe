<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jlwoolf.android.tictactoe" >

    <application
        android:name="com.jlwoolf.android.tictactoe.TTTApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TTT" >
        <activity
            android:name="com.jlwoolf.android.tictactoe.Navigation2Activity"
            android:label="@string/title_activity_navigation2"
            android:theme="@style/Theme.TTT.NoActionBar" >
        </activity>
        <activity android:name="com.jlwoolf.android.tictactoe.ui.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>